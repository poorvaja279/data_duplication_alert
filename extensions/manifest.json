{
  "manifest_version": 3,
  "name": "Download Duplicate Alert",
  "version": "1.0",
  "description": "Detects duplicate file downloads.",
  "permissions": ["downloads", "storage"],
  "host_permissions": ["http://localhost:5000/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}